
page {
  background: #f6f6f6;
}

.main {
  background: #fff;
  margin-bottom: 20rpx;
  margin-top: 20rpx; 
}

.cell,.cell_height {
  display: flex;
  padding: 10rpx 20rpx 10rpx 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  width: 710rpx;
  border-bottom:1px solid #eeeeee 
}
.cell_height{
 height: 240rpx;
}

.cell_title {
  margin-left: 20rpx;
  width: 180rpx;
  font-size: 30rpx;
  color: #ccc;
}
.mini_text{
  font-size: 28rpx;
  line-height: 30rpx;
  margin-top:16rpx; 
}

.cell_body {
  width: 470rpx;
  font-size: 30rpx;
}
.cell_body_center {
  width: 270rpx;
  font-size: 30rpx;
}
.cell_body_right {
  width: 200rpx;
  font-size: 30rpx;
}
.cell_body_textarea{
  font-size: 30rpx;
  line-height: 30rpx;
  width: 100%;
  height: 100%;
  resize: none;
}
.cell_body_title {
  height: 60rpx;
  margin: 10rpx;
  width: 100%;
  font-size: 30rpx;
}
.RedBtn{
  background: #FF6060;
  border: none;
  margin: 20rpx;
  color: #fff;
}
.RedBtn:hover{
    background: #FF3030;
}
.cell_body_imgAdd{
  width: 50rpx;
  height: 50rpx;
  margin: 7rpx;
  padding: 5rpx;
  border: 3rpx solid #bfbfbf;
  border-radius: 10rpx;

}
.CenterTips{
  text-align: center;
  margin: 20rpx;
  color: #ccc;
}